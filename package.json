{
	"name": "fn-context",
	"version": "0.0.1",
	"description": "",
	"private": false,
	"main": "./src/index.ts",
	"scripts": {
		"start": "node --loader ts-node/esm ./test/index.ts",
		"build": "npx node-build-engine",
		"npm:publish": "npm run build && npm publish --access public"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^20.14.10",
		"esm": "^3.2.25",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.5.3"
	},
	"browserify": {
		"entries": "./index.ts",
		"standalone": "MyLibrary",
		"ignore": [
			"some-module"
		],
		"insertGlobals": false,
		"detectGlobals": true,
		"ignoreMissing": false,
		"debug": true,
		"extensions": [
			".ts",
			".js"
		],
		"noParse": [
			"large-library"
		],
		"externalRequireName": "require"
	},
	"dependencies": {
		"node-build-engine": "^1.3.2"
	}
